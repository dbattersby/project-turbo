<div id="<%= dom_id project %>">
  <p>
    <strong>Name:</strong>
    <%= project.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= project.description %>
  </p>

  <p>
    <strong>Due date:</strong>
    <%= project.due_date %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= project.status %>
